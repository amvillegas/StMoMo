constRHfix_ax <- function(ax, bx, kt, b0x, gc, wxt, ages){
c1 <- sum(bx, na.rm = TRUE)
bx <- bx / c1
kt <- kt * c1
list(ax = ax, bx = bx, kt = kt, b0x = b0x, gc = gc)
}
RHfix_ax <- StMoMo(link = "log", staticAgeFun = FALSE,
periodAgeFun = "NP", cohortAgeFun = "1",
constFun =  constRHfix_ax)
ages.fit <- 65:95
years.fit <- 1950:2000
ax <- rowMeans(log(EWMaleData$Dxt / EWMaleData$Ext)[as.character(ages.fit), as.character(years.fit)])
wxt <- genWeightMat(ages.fit,  years.fit, clip = 3)
RHfix_axfit <- fit(RHfix_ax, Dxt = Dxt, Ext = Ext,
ages =ages, years = years,
ages.fit = ages.fit, yyears.fit = years.fit,
oxt = ax, wxt = wxt)
Ext <-FRdata$pop$male
Dxt <-FRdata$rata$male * Ext
ages <- FRdata$age
years <- FRdata$year
Dxt <-FRdata$rate$male * Ext
ages <- FRdata$age
years <- FRdata$year
ages.fit <- 65:95
years.fit <- 1950:2000
ax <- rowMeans(log(EWMaleData$Dxt / EWMaleData$Ext)[as.character(ages.fit), as.character(years.fit)])
ax <- rowMeans(log(Dxt / Ext)[as.character(ages.fit), as.character(years.fit)])
wxt <- genWeightMat(ages.fit,  years.fit, clip = 3)
RHfix_axfit <- fit(RHfix_ax, Dxt = Dxt, Ext = Ext,
ages =ages, years = years,
ages.fit = ages.fit, yyears.fit = years.fit,
oxt = ax, wxt = wxt)
wxt <- genWeightMat(ages.fit,  years.fit, clip = 3)
RHfix_axfit <- fit(RHfix_ax, Dxt = Dxt, Ext = Ext,
ages =ages, years = years,
ages.fit = ages.fit, years.fit = years.fit,
oxt = ax, wxt = wxt)
